
config zerotier
	option enabled 0

	# persistent configuration folder (for ZT controller mode)
	#option config_path '/etc/zerotier'

	#option port '9993'

	# path to the local.conf
	#option local_conf '/etc/zerotier.conf'

	# Generate secret on first start
	option secret ''

	# join networks on startup
	#list join '8056c2e21c000001'
	#list join '<other_network>'

# for /etc/init.d/zerotier join|leave earth
config network 'earth'
	option id '8056c2e21c000001'
	option join_at_startup 1
